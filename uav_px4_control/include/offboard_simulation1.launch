<launch>


  <!-- Python nodes -->
  <node name="waypoints" pkg="uav_control" type="waypoints.py" />

  <!-- C++ nodes -->
  <node name="offb_node" pkg="uav_control" type="offb_node" />

  <!-- Run Rosboard for data visualization -->
  <!-- <node name="rosboard_node" pkg="rosboard" type="rosboard_node" /> -->

  <!-- Include mavros in the simulation -->
  <include file="$(find mavros)/launch/px4.launch">
  <arg name="fcu_url" default="udp://:14540@127.0.0.1:14557"/>
  </include>

</launch>
